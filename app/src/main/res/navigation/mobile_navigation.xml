<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.genengankoi.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_product"
        android:name="com.example.genengankoi.ui.product.ProductFragment"
        android:label="@string/title_product"
        tools:layout="@layout/fragment_product"/>

    <fragment
        android:id="@+id/navigation_lelang"
        android:name="com.example.genengankoi.ui.lelang.LelangFragment"
        android:label="@string/title_lelang"
        tools:layout="@layout/fragment_lelang" />

    <fragment
        android:id="@+id/navigation_rekpen"
        android:name="com.example.genengankoi.ui.rekpen.RekpenFragment"
        android:label="@string/title_rekpen"
        tools:layout="@layout/fragment_rekpen" />

    <fragment
        android:id="@+id/navigation_akun"
        android:name="com.example.genengankoi.ui.akun.AccountFragment"
        android:label="Akun"
        tools:layout="@layout/account_fragment" />

</navigation>